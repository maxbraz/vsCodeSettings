{
    "window.zoomLevel": 2,
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "vscode-icons",
    "eslint.nodePath": "/usr/local/bin/eslint",
    "eslint.options": {
        "configFile": "/Users/Max/.eslintrc",
        "autoFixOnSave": true
